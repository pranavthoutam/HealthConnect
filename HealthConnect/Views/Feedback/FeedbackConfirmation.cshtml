@{
    ViewData["Title"] = "Feedback Submitted";
}
<!DOCTYPE html>
<html>
<head>
    <title>Feedback Submitted</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body class="bg-light">
    <div class="container mt-5">
        <div class="alert alert-success text-center shadow-lg p-4">
            <h4>@TempData["FeedbackMessage"]</h4>
            <a asp-action="ProfileDashboard" asp-controller="UserProfile" class="btn btn-primary mt-4">Back to Dashboard</a>
        </div>
    </div>
</body>
</html>
